APP_NAME=UserHub
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stderr
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=db
DB_PORT=5432
DB_DATABASE=userhub
DB_USERNAME=userhub
DB_PASSWORD=secret

CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

FILESYSTEM_DISK=public
FILESYSTEM_PUBLIC_URL=${APP_URL}/storage

JWT_SECRET=

MAIL_MAILER=log
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=no-reply@userhub.test
MAIL_FROM_NAME="UserHub"

RUN_MIGRATIONS=true

# Optional: configure timezone and locale
APP_TIMEZONE=UTC
APP_LOCALE=en
